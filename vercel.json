{
  "rewrites": [
    { "source": "/socket.io/:path*", "destination": "/api/socketio/:path*" }
  ],
  "headers": [
    {
      "source": "/socket.io/:path*",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Connection", "value": "keep-alive" }
      ]
    }
  ]
}
